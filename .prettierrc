{
  "semi": false,
  "tabWidth": 2,
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/(?!.*\\.(css|scss|png|jpg|jpeg|gif|svg)$)(.*)$",
    "^(?!.*\\.(css|scss|png|jpg|jpeg|gif|svg)$)[./]",
    "",
    "^@/(.*)(png|jpg|jpeg|gif|svg)$",
    "^.+\\.(png|jpg|jpeg|gif|svg)$",
    "^.+\\.(css|scss)$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
